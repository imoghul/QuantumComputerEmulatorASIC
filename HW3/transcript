# vsim -voptargs="+acc" -do "log -r *" divide_by_two_tb 
# Start time: 16:10:02 on Sep 13,2023
# ** Note: (vsim-3813) Design is being optimized due to module recompilation...
# Loading work.divide_by_two_tb(fast)
# Loading work.counter(fast)
# log -r *
run 200ns
add wave -position insertpoint  \
sim:/divide_by_two_tb/dec
add wave -position insertpoint  \
sim:/divide_by_two_tb/clock100 \
sim:/divide_by_two_tb/latch \
sim:/divide_by_two_tb/dec \
sim:/divide_by_two_tb/divide_by_two \
sim:/divide_by_two_tb/in \
sim:/divide_by_two_tb/zero
